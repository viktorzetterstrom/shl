{
  "AWSEBDockerrunVersion": "2",
  "containerDefinitions": [
    {
      "name": "client",
      "image": "viktorzetterstrom/shl-client",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "api",
      "image": "viktorzetterstrom/shl-api",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "proxy",
      "image": "viktorzetterstrom/shl-proxy",
      "essential": true,
      "portMappings": [{ "hostPort": 80, "containerPort": 80 }],
      "links": ["client", "api"]
    }
  ]
}
